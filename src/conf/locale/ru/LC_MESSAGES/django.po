msgstr "поддерживает"

#: src/flavors/cycle1/_config.translations.py:106
msgid "About"
msgstr "О"

#: src/flavors/cycle1/_config.translations.py:107
msgid "Boston PB Home"
msgstr "Бостон ПБ Главная"

#: src/flavors/cycle1/_config.translations.py:108
msgid "Filter Ideas"
msgstr "Фильтровать идеи"

#: src/flavors/cycle1/_config.translations.py:109
